<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="STRIVE.Trains.WithInventory.YogaTrainWith"
             Title="YogaTrainWith">
    
    <ScrollView>
        <VerticalStackLayout>
            <ImageButton Source="dumbbellsdividing.png"
                     Clicked="TreePose"
         Margin="20,30,20,60"
         />
            <ImageButton Source="upperpress.png"
                     Clicked="ChairPose"
         Margin="20,0,20,60"
         />
            <ImageButton Source="bicepsonknee.png"
                     Clicked="MountainPose"
         Margin="20,0,20,60"
         />
            <ImageButton Source="tricepsstretching.png"
                     Clicked="StarPose"
         Margin="20,0,20,60"
         />
            <ImageButton Source="tricepsstretching.png"
                    Clicked="WarriorPose"
         Margin="20,0,20,60"
         />
        </VerticalStackLayout>
    </ScrollView>
</ContentPage>